{
    "title": "Gitbook入门",
    "author": "Wang Yuanwei <now_way@126.com>",
    "description": "gitbook入门",
    "language": "zh-hans",
    "gitbook": ">=3.0.0",
    "plugins": [
        "disqus",
        "anchors",
        "multipart",
        "-search",
        "-lunr",
        "search-plus",
        "-sharing",
        "splitter",
        "sectionx",
        "simple-page-toc",
        "tbfed-pagefooter"
    ],
    "pluginsConfig": {
        "disqus": {
            "shortName": "wr-gitbook"
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright &copy imwyw",
            "modify_label": "最后修改时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
    }
}
