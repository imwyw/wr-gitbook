{
    "title": "Gitbook入门",
    "author": "Wang Yuanwei <now_way@126.com>",
    "description": "gitbook入门",
    "language": "zh-hans",
    "plugins": [
        "disqus",
        "anchors",
        "multipart",
        "anchors",
        "codeblock-filename",
        "-search",
        "search-pro",
        "ace",
        "tbfed-pagefooter"
    ],
    "pluginsConfig": {
        "disqus": {
            "shortName": "wr-gitbook"
        },
        "search-pro": {
            "cutWordLib": "nodejieba",
            "defineWord": ["Gitbook Use"]
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright &copy 2017",
            "modify_label": "最后修改时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
    }
}
